# API Keys - REQUIRED
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Database Configuration
REDIS_URL=redis://redis-primary:6379
MONGODB_URL=mongodb://admin:multi_agent_pass_2024@mongodb:27017/multi_agent_conversations?authSource=admin
POSTGRES_URL=postgresql://agent_user:agent_analytics_2024@postgres:5432/multi_agent_analytics

# n8n Configuration
N8N_WEBHOOK_URL=http://n8n:5678/webhook
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=multi_agent_n8n_2024

# Application Configuration
ENVIRONMENT=development
LOG_LEVEL=DEBUG
JWT_SECRET=your_jwt_secret_key_here
CORS_ORIGINS=http://localhost:3000,http://localhost:5678

# Voice Processing
TTS_CACHE_DURATION=86400
STT_MODEL=whisper-1
DEFAULT_VOICE_PROVIDER=elevenlabs

# Character Configuration
CHARACTER_ADAPTATION_RATE=0.1
MEMORY_CONSOLIDATION_THRESHOLD=0.7
CONVERSATION_CONTEXT_WINDOW=4000

# Safety and Moderation
CONTENT_MODERATION_ENABLED=true
SAFETY_CONFIDENCE_THRESHOLD=0.7
HUMAN_REVIEW_QUEUE_SIZE=100

# Performance
MAX_CONCURRENT_CONVERSATIONS=50
REDIS_CONNECTION_POOL_SIZE=20
MONGODB_CONNECTION_POOL_SIZE=10

# Monitoring and Analytics
ENABLE_ANALYTICS=true
METRICS_EXPORT_INTERVAL=300
HEALTH_CHECK_INTERVAL=30

# Development Tools (only for dev environment)
ENABLE_DEBUG_ENDPOINTS=true
MOCK_AI_RESPONSES=false
LOG_CONVERSATION_CONTENT=false